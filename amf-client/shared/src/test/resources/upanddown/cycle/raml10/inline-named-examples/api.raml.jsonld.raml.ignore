#%RAML 1.0
title: test
types:
  Organization: !include data-types/organization-data-type.raml
/organizations:
  put:
    body:
      application/json:
        examples:
          value:
            -
              category:
                id: 39691
                name: Religious and Spiritual
              created_at: 2013-01-02T22:21:34Z
            -
              category:
                id: 61111
                name: Service
              created_at: 2016-02-01T16:37:02Z
        items: Organization
  get:
    displayName: Get organizations
    responses:
      "200":
        body:
          application/json:
            examples:
              value:
                -
                  category:
                    id: 39691
                    name: Religious and Spiritual
                  created_at: 2013-01-02T22:21:34Z
                -
                  category:
                    id: 61111
                    name: Service
                  created_at: 2016-02-01T16:37:02Z
            items: Organization
  /{organization_id}:
    get:
      displayName: Get organizations by organization ID
      responses:
        "200":
          body:
            application/json:
              examples:
                value:
                  alternate_id: 1
                  category:
                    id: 39691
                    name: Religious and Spiritual
                  created_at: 2013-01-02T22:21:34Z
              type: Organization
    uriParameters:
      organization_id:
        type: number
        description: The organization's specific ID
