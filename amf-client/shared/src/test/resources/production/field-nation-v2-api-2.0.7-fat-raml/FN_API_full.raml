#%RAML 1.0
title: Field Nation V2 API
version: 2.0.0
baseUri: 'http://api.fieldnation.com/api/rest/v2'
mediaType:
  - application/json
  - application/x-www-form-urlencoded
  - multipart/form-data
protocols:
  - HTTPS
uses:
  fn-misc: lib/misc_types.raml
annotationTypes:
  oas-summary:
    type: string
    allowedTargets: Method
  oas-responses-default:
    type: any
    allowedTargets: Method
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-body-name:
