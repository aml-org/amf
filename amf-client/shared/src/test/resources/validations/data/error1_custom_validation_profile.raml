#%Validation Profile 1.0

profile: Test Profile

description: Test profile that ignores validations about schemes

extends: RAML

info:
  - my-custom-validation

disabled:
  - amf-parser.raml-schemes

validations:

  my-custom-validation:
    message: error wadus
    targetClass: schema.WebAPI
    propertyConstraints:
      http.scheme:
        in: [WADUS]