    description: Returns a list of work orders.
    (oas-summary): Get Work Orders
    (oas-responses-default):
      body:
        application/json:
          type: fn-misc.Error
      description: Unexpected error
    responses:
      '200':
        body:
          application/json:
            type: fn-misc.WorkOrders
        description: Work orders
    queryParameters:
      list:
        description: 'Saved group which sandboxes filters, sorts and column preferences in addition to pre-applied settings (e.g.: tabs or saved searches)'
        required: false
        type: string
      columns:
        description: Specify the columns and their order to return data for
        required: false
        type: string
      page:
        description: Specify the page number
        required: false
        type: integer
      per_page:
        description: Specify the results per page
        required: false
        type: integer
      view:
        description: 'Caters the response data to how you''re going to display it. For most uses, you''ll want ''list''; other options might include map, schedule, tiles, etc.'
        required: false
        type: string
      sticky:
        description: 'Persists filters, columns, and sorts (all but pagination) to re-apply on future requests where omitted. Persists to the ''list'' specified for the authenticated user'
        required: false
        type: boolean
        default: false
      sort:
        description: Sorts results by a specified column id
        required: false
        type: string
      order:
        description: Whether the sort should be ascending or descending
        required: false
        type: string
        enum:
          - asc
          - desc
      f_:
        description: Clears all filters
        required: false
        type: string
      f_max_approval_time:
        description: (Provider) Filters work to where company has specified their maximum approval day target is less than or equal to provided number of days
        required: false
        type: integer
      f_rating:
        description: '(Provider) Filters work by buyer''s star rating (Buyer) Filters work by assigned provider''s star rating -- value can be a single number (e.g.: 3.5) where that value must be met or exceeded or a specific inclusive range (e.g.: 3.5,5.0)'
        required: false
        type: string
      f_flightboard_tomorrow:
        description: (Provider) Used by the confirm tomorrow work screen
        required: false
        type: boolean
      f_requests:
        description: '(Buyer) Filters work with requests (e.g.: true) or with no requests (e.g.: false)'
        required: false
        type: boolean
      f_counter_offers:
        description: '(Buyer) Filters work with counter-offer requests of any type (schedule, expense or pay change) when true (e.g.: true) or with no counter offers (e.g.: false)'
        required: false
        type: boolean
      f_work_order_id:
        description: (Buyer/Provider) Filters work by specific work order ids
        required: false
        type: string
      f_hourly:
        description: '(Buyer/Provider) Filters work by that of a minimum hourly rate (includes only hourly or blended pay structure) -- may be given a single value which must be met or exceeded (e.g.: 50) or an inclusive range (e.g.: 35,75)'
        required: false
        type: string
      f_fixed:
        description: '(Buyer/Provider) Filters work by that of a minimum fixed rate (includes only fixed or blended pay structure) -- may by given a single value which must be met or exceeded (e.g.: 250) or an inclusive range (e.g.: 250,500)'
        required: false
        type: string
      f_device:
        description: '(Buyer/Provider) Filters work by that of a minimum per-device rate (includes only device pay structure) -- may by given a single value which must be met or exceeded (e.g.: 250) or an inclusive range (e.g.: 250,500)'
        required: false
        type: string
      f_pay:
        description: '(Buyer/Provider) Filters work by that whose minimum or maximum payout meets or exceeds a single value (e.g.: 250) or whose minimum or maximum payout falls between an inclusive range (e.g.: 250,500)'
        required: false
        type: string
      f_templates:
        description: '(Buyer) Filters work orders created using a given template, provided either by name or it''s unique id. May be given as a single value or as a list of comma-separated values. If the template name itself has a comma, you should use its unique identifier instead'
        required: false
        type: string
      f_type_of_work:
        description: '(Buyer/Provider) Filters work by that of a given type (e.g.: Networking). May be given a single or comma separated list of values of the full descriptions (Networking) or their unique identifiers (40) -- if the descriptions include commas you should use their unique identifiers'
        required: false
        type: string
      f_time_zone:
        description: '(Buyer/Provider) Filters work by that of a given time zone (e.g.: America/New_York), list of time zones (e.g.: America/New_York, America/Chicago), its offset (e.g.: -5) or a range of offsets (e.g.: -4,-6)'
        required: false
        type: string
      f_mode:
        description: '(Buyer/Provider) Filters work by that of a given mode which must be one of the following values: ''managed'', ''marketplace'', or ''any'''
        required: false
        type: string
        enum:
          - managed
          - marketplace
          - any
      f_company:
        description: '(Buyer/Provider) Filters work by buyer, can be given text which would free-form search all buyers by name (e.g.: ''acme''), or a company''s unique identifier (e.g.: ''50''). You can also include a list of unique identifiers (e.g.: 50,51,52) to search for multiple buyers'
        required: false
        type: string
      f_worked_with:
        description: (Provider) Filters work orders to include only those by company's I or my vendor/service company has previously worked with
        required: false
        type: string
      f_manager:
        description: '(Buyer) Filters work to that assigned to a given company user, either by name (case insensitive), email or their unique id or a list of comma separated values of any of these'
        required: false
        type: string
      f_client:
        description: '(Buyer) Filters work to that assigned to a given client company, either by name (case insensitive), or their unique id or a list of comma separated values of either of these'
        required: false
        type: string
      f_project:
        description: '(Buyer) Filters work to that assigned to a given project, either by name (case insensitive), or their unique id or a list of comma separated values of either of these'
        required: false
        type: string
