{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/schema",
  "definitions": {
    "schema": {
      "title": "Family Group",
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": true,
        "properties": {
          "name": {
            "type": "string",
            "minLength": 3
          },
          "age": {
            "type": "integer",
            "minimum": 0,
            "multipleOf": 1
          }
        }
      }
    },
    "Person": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "name": {
          "type": "string",
          "minLength": 3
        },
        "age": {
          "type": "integer",
          "minimum": 0,
          "multipleOf": 1
        }
      }
    }
  }
}
