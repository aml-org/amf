  '/{work_order_id}':
    displayName: '{work_order_id}'
    /requests:
      displayName: requests
      '/{request_id}':
        displayName: '{request_id}'
        uriParameters:
          request_id:
            description: ID of work order request/counter offer
            required: true
            type: integer
        get:
          displayName: getRequestByWorkOrderAndRequest
          description: Get request/ counter offer for assigned work order.
          (oas-summary): Get Request/Counter offer by Work Order
          (oas-responses-default):
            body:
              application/json:
                type: fn-misc.Error
            description: Unexpected error
          responses:
            '200':
              body:
                application/json:
                  type: fn-misc.Request
              description: Gets requests / counter offers for a work order
          queryParameters:
            async:
              description: Async
              required: false
              type: boolean
            acting_user_id:
              description: Acting user id
              required: false
              type: integer
          (oas-tags):
            - workorder
        delete:
          displayName: deleteRequestByWorkOrder
          description: Removes or hides a request by a user from a work order
          (oas-summary): Delete Request by Work Order
          (oas-responses-default):
            body:
              application/json:
                type: fn-misc.Error
            description: Unexpected error
          responses:
            '200':
              description: Success
          queryParameters:
            async:
              description: Async
              required: false
              type: boolean
            acting_user_id:
              description: Acting user id
              required: false
              type: integer
          (oas-tags):
            - workorder
      post:
        displayName: requestByWorkOrder
        description: Request or un-hide a request for a work order
        (oas-summary): Request by Work Order
        body:
          application/json:
            type: fn-misc.Request
            (oas-body-name): request
            description: JSON Model
        (oas-responses-default):
          body:
            application/json:
              type: fn-misc.Error
          description: Unexpected error
        responses:
          '200':
            body:
              application/json:
                type: fn-misc.WorkOrder
            description: Success
        queryParameters:
          async:
            description: Async
            required: false
            type: boolean
        (oas-tags):
          - workorder
      get:
        displayName: getRequestsByWorkOrder
        description: Returns a list of work order requests or counter offers requested by the assigned provider.
