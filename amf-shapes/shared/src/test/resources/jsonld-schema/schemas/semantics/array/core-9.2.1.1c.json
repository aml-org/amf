{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "type": "array",
  "@context": {
    "alias": "amlorg://a.ml/semantics.yaml#",
    "@characteristics": [
      "alias:rootArray"
    ]
  },
  "items": {
    "@context": {
      "@characteristics": [
        "alias:allOfNode"
      ]
    },
    "allOf": [
      {
        "@context": {
          "@characteristics": [
            "alias:numberScalar"
          ]
        },
        "type": "number"
      },
      {
        "type": "integer"
      }
    ]
  }
}
