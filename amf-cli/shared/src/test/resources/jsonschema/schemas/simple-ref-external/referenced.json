{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Computer",
  "type": "object",
  "definitions": {
    "processor": {
      "type": "object",
      "properties": {
        "frequency": {
          "type": "string",
          "enum": ["3.2ghz", "3.4ghz"]
        },
        "socket": {
          "type": "string"
        }
      }
    },
    "RAM": {
      "type": "object",
      "properties": {
        "memory": {
          "type": "string",
          "enum": ["8GB", "16GB", "32GB"]
        },
        "frequency": {
          "type": "integer"
        }
      }
    }
  }
}