        "amf-client" [style=dashed]
	    "amf-webapi"
	    color=gray
    }
    subgraph cluster_1 {
        "amf-custom-validation"
    	"amf-aml" [style=dashed]
	    "amf-validation"
	    color=gray
    }
    
    subgraph cluster_2 {
