#%RAML 1.0
title: API
version: 1.0

resourceTypes:
  MyRT:
    get:
      responses:
        200:
          body:
            application/json:
              type: <<typeParam>>
              example: <<exampleParam>>


/endpoint:
  type:
    MyRT:
      typeParam:
        type: object
        properties:
          name:
            type: string
            required: yes #should be 'true'
          lastName: string
      exampleParam: !include example.json