      securedBy:
        -
          Oauth2:
            scopes:
              - https://www.googleapis.com/auth/prediction
  post:
    displayName: prediction.training.insert
    description: Begin training your model
    body:
      application/json:
        type: object
        (oas-body-name): body
        additionalProperties: true
        properties:
          id:
            type: string
            required: false
          kind:
            default: prediction#training
