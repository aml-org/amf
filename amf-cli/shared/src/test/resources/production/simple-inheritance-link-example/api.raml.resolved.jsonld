#%RAML 1.0
title: Sample API
/personas:
  get:
    responses:
      "200":
        body:
          application/json:
            type: object
            properties:
              idNumber:
                type: integer
              firstName:
                type: string
              middleName?:
                type: string
              lastName:
                type: string
              gender:
                enum:
                  - F
                  - M
            example:
              idNumber: 35877877
              firstName: Jose
              middleName: Ramiro
              lastName: Funes Mori
              gender: M
