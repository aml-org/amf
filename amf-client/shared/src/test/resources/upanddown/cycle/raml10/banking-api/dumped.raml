#%RAML 1.0
title: test
version: "1.0"
mediaType: application/json
baseUri: falsedomain.com/apis
/customers:
  /{customer_id}:
    uriParameters:
      customer_id:
        required: true
    get:
      description: Returns Customer data
      responses:
        "200":
          body:
            application/json:
              type: any
    delete:
      description: Removes a Customer from the system
    /accounts:
      get:
        description: Returns a collection accounts
        responses:
          "200":
            body:
              application/json:
                type: any
    /cards:
      /debit:
        get:
          description: Returns a collection of cards
          responses:
            "200":
              body:
                application/json:
                  type: any
        post:
          description: Requests the creation of a new DebitCard
          body:
            application/json:
              type: string
