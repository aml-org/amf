#%RAML 1.0
title: test
baseUri: test.com/accounts
types:
  User:
    properties:
      name:
        type: string
      age:
        type: integer
      account:
        type: object
  Account:
    properties:
      accountId:
        type: string
/users:
  get:
    responses:
      "200":
        body:
          application/json:
            items: User
  /{id}:
    get:
      responses:
        "200":
          body:
            application/json: User
    /account:
      get:
        responses:
          "200":
            body:
              application/json: Account
    uriParameters:
      id:
        type: string
protocols:
  - http
