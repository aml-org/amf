              input:
                type: object
                additionalProperties: true
                properties:
                  csvInstance:
                    type: array
                    items: {}
                    required: false
                required: false
        responses:
          "200":
            body:
              application/json:
                additionalProperties: true
                properties:
                  id:
                    type: string
                    required: false
                  kind:
                    default: prediction#output
                    type: string
                    required: false
                  outputLabel:
                    type: string
                    required: false
                  outputMulti:
                    type: array
                    items:
                      type: object
                      additionalProperties: true
                      properties:
                        label:
                          type: string
                          required: false
