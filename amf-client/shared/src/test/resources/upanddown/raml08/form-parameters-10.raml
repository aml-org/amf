#%RAML 1.0
title: Example API
/resource:
  /{resourceId}:
    post:
      body:
        application/x-www-form-urlencoded:
          additionalProperties: false
          properties:
            name?:
              anyOf:
                -
                  description: The name of the resource to create
                  type: string
                  example: Comment
                -
                  description: The name 1 of the resource to create
                  type: string
                  example: Comment1
            description?:
              description: A description of the resource to create
              type: string
              example: User-generated content pertinent to the associated blog post
            default?:
              description: a type without type for set default
              type: string
