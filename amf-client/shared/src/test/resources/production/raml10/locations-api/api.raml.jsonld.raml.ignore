#%RAML 1.0
baseUri: https://mocksvc.mulesoft.com/mocks/e5686b9d-885a-4be9-b95f-f3cf607eb467
title: Location API
version: "1.0"
/locations:
  get:
    description: Returns a collection of location.Locations
    responses:
      "200":
        body:
          examples:
            blah:
              -
                id: 8624
                addressType: STORE
                country: USA
                state: CA
                city: San Francisco
                street_address: 610 Old Mason Street
                zip: 94129
              -
                id: 8625
                addressType: STORE
                country: USA
                state: CA
                city: San Francisco
                street_address: 1590 Bryant St
                zip: 94103
              -
                id: 8626
                addressType: STORE
                country: USA
                state: CA
                city: Berkeley
                street_address: 2727 Milvia St
                zip: 94703
          type: location.Locations
/str:
  get:
    description: Returns a collection of location.Locations
    responses:
      "200":
        body:
          type: location.str
/obj:
  get:
    description: Returns a collection of location.Locations
    responses:
      "200":
        body: location.obj
/arr:
  get:
    description: Returns a collection of location.Locations
    responses:
      "200":
        body: location.arr
uses:
  location: exchange_modules/a6d4cdc7-ecb0-4315-a6a6-216c71ba0b54/locations-datatypes/1.0.2/locations-datatypes.raml
mediaType: application/json
