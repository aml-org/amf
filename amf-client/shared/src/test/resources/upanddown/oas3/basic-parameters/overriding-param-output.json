{
  "openapi": "3.0.0",
  "info": {
    "title": "Basic Callbacks",
    "version": "0.1"
  },
  "paths": {
    "/test/{itemId}": {
      "get": {
        "parameters": [
          {
            "in": "query",
            "name": "someNumber",
            "allowEmptyValue": true,
            "allowReserved": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "path",
            "name": "itemId",
            "deprecated": false,
            "schema": {
              "type": "string",
              "x-amf-examples": {
                "example": "some example"
              }
            },
            "style": "simple"
          },
          {
            "in": "cookie",
            "name": "aCookie",
            "schema": {
              "type": "string"
            },
            "style": "form"
          }
        ],
        "summary": "this is a test",
        "operationId": "test",
        "responses": {
          "200": {
            "description": "description",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}
