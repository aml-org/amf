#%RAML 1.0
title: test
version: 1
baseUri: https://api.domain.com/{version}
securitySchemes:
    invalid:
        type: OAuth 2.0
        settings:
          authorizationGrants: [ authorization_code ]

    valid:
        type: OAuth 2.0
        settings:
          authorizationUri: https://www.domain.com/1/oauth2/authorize
          accessTokenUri: https://www.domain.com/1/oauth2/token
          authorizationGrants: [ authorization_code ]
